{
  "name": "node-tensorflow",
  "version": "1.1.0",
  "description": "Node Tensorflow",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/omuryildirim/before-evening",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "start": "tsc -p tsconfig.json -w",
    "build": "tsc -p tsconfig.json",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^2.4.0",
    "@tensorflow/tfjs-node": "^2.4.0",
    "before-evening-game-engine": "workspace:*",
    "shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!test",
      "!build/module/**"
    ],
    "ignoredByWatcher": [
      "test"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
